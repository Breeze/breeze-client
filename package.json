{
  "name": "breeze-client",
  "version": "2.0.8",
  "private": true,
  "description": "Breeze data management for JavaScript clients",
  "repository": "https://github.com/Breeze/breeze-client.git",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^8.2.10",
    "@angular/compiler": "^8.2.10",
    "@angular/compiler-cli": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@types/jest": "^25.1.1",
    "@types/node": "^12.12.26",
    "canonical-path": "^1.0.0",
    "fs-extra": "^9.0.0",
    "ng-packagr": "^9.1.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.3",
    "tslint": "^6.1.1",
    "typedoc": "^0.16.9",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "build": "npm run clean && npm run packagr && npm pack ./src/dist && npm run install-to-spec",
    "test": "npm run test --prefix ./spec",
    "clean": "rimraf ./temp && rimraf ./src/*.js && rimraf ./src/*.d.ts && rimraf ./src/dist && rimraf node_modules/breeze-client.js",
    "install-to-spec": "cd ./spec && npm install ../breeze-client-2.0.8.tgz",
    "publish": "npm publish --tag latest ./src/dist",
    "tsc": "tsc --build ./src/tsconfig.json",
    "watch-tsc": "tsc --build ./src/tsconfig.json -w",
    "typedoc": "typedoc --out ./temp/docs/ --exclude xx  --excludePrivate --excludeNotExported  --tsConfig ./src/tsconfig.typedoc.json --mode file --readme ./readme.doc.md ./src/breeze.ts && node ./tools/copy-if-different.js ./temp/docs ./docs ",
    "packagr": "ng-packagr -p ./src/package.json"
  },
  "author": {
    "name": "IdeaBlade",
    "email": "jayt@ideablade.com",
    "url": "https://www.ideablade.com/"
  },
  "contributors": [
    "Jay Traband",
    "Steve Schmitt",
    "Marcel Good",
    "Ward Bell"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Breeze/breeze-client/issues"
  },
  "homepage": "https://github.com/Breeze/breeze-client#readme"
}
